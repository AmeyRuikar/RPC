#include "file.h"

double *
sub_1_svc(char **argp, struct svc_req *rqstp)
{
	static double  result;

	/*
	 * insert server code here
	 */
	 
	 FILE * fp;
	 char	str[100];
	 char	ch;
	 int	j = 0;
	 int	len;
	 
	 strcpy(str, *argp);
	 result = 0;
	 fp = fopen("test.txt","r");
	 
	 while(1){
	 
	 	ch = fgetc(fp);
	 	
	 	if(ch == 0 || ch == EOF)
	 		break;
	 		
	 	if(ch == str[j]){
	 		len++;
	 		j++;
	 	}
	 	else{
	 		len = 0;
	 		j = 0;
	 	}
	 	
	 	if(len == strlen(str))
	 	{
			result = result + 1;
		}
	 
	 
	 }
	 
	 

	return &result;
}
